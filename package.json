{
  "name": "@said/wl-composable-module",
  "version": "1.0.22-alpha.6",
  "description": "Whitelabel Composable module nuxt",
  "repository": "https://github.com/arimaulana07/wl-composable-module",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://git.tbk.co.id/api/v4/projects/759/packages/npm/"
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.0",
    "@vueuse/core": "^11.1.0",
    "@vueuse/nuxt": "^11.1.0",
    "fuse.js": "^7.0.0",
    "pinia": "^2.2.2",
    "vue": "^3.5.8",
    "vue3-toastify": "^0.2.3"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.9.0",
    "@nuxt/test-utils": "^3.9.0",
    "@pinia/nuxt": "^0.5.4",
    "@types/node": "^20.10.6",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "nuxt": "^3.9.0",
    "vitest": "^2.1.1"
  }
}
